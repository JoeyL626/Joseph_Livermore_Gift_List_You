{
  "manifest_version": 2,

  "name": "Gift List You",
  "description": "This extension for makeing a gift lists",
  "version": "1.0",
  "permissions": [
      "tabs"
   ],
  "content_scripts": [
    {
    	"matches": ["http://www.google.com/*"],
      "js": ["jquery-3.1.0.min.js", "popup.js"]
    }
  ],

  "browser_action": {
    "default_popup": "popup.html"
  }
}
